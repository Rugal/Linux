# Source global definitions


alias c="clear"
alias ll="ls -lFhp"
alias l="ls -p"
alias cl="clear;ll;pwd"
alias lc=cl
alias df="df -h"
alias free="free -m"
alias ccd="cd .."
alias cdd="cd -"
alias dstat="dstat -cdlmnpsy"
alias slurm="slurm -i wlan0"
alias t="tig"
alias lg="lazygit"
alias ps="ps -ef|grep"
alias v="nvim"
alias V="v"
alias n="v"
alias g="git"
alias matrix="cmatrix -sb"
alias m="mvnd"
